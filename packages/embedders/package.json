{
  "name": "@repolens/embedders",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./openai": {
      "types": "./dist/openai-embedder.d.ts",
      "import": "./dist/openai-embedder.js",
      "require": "./dist/openai-embedder.cjs",
      "default": "./dist/openai-embedder.js"
    }
  },
  "scripts": {
    "build": "tsup src/openai-embedder.ts --format cjs,esm --dts",
    "dev": "tsup src/openai-embedder.ts --format cjs,esm --dts --watch"
  },
  "dependencies": {
    "openai": "^4.89.0",
    "gpt-tokenizer": "^2.9.0",
    "@repolens/types": "workspace:*"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@turbo/gen": "^2.4.4",
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "tsup": "^8.4.0",
    "typescript": "5.8.2"
  }
}
