{
  "name": "@repolens/types",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./fetcher": {
      "types": "./dist/fetcher.d.ts",
      "import": "./dist/fetcher.mjs",
      "require": "./dist/fetcher.js",
      "default": "./dist/fetcher.mjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "5.8.2"
  }
}
