{
  "name": "@repo-vector/core",
  "version": "0.0.0",
  "main": "./dist/repo-vector.js",
  "module": "./dist/repo-vector.mjs",
  "types": "./dist/repo-vector.d.ts",
  "exports": {
    ".": {
      "types": "./dist/repo-vector.d.ts",
      "require": "./dist/repo-vector.js",
      "import": "./dist/repo-vector.mjs"
    }
  },
  "scripts": {
    "build": "tsup src/repo-vector.ts --format cjs,esm --dts",
    "dev": "tsup src/repo-vector.ts --format cjs,esm --dts --watch"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@turbo/gen": "^2.4.4",
    "@types/node": "^22.13.10",
    "dotenv": "^16.4.7",
    "eslint": "^9.22.0",
    "tsup": "^8.4.0",
    "typescript": "5.8.2",
    "ts-morph": "^25.0.1"
  },
  "dependencies": {
    "@repo-vector/github-fetcher": "workspace:*",
    "@repo-vector/parser": "workspace:*",
    "@repo-vector/parser-default": "workspace:*",
    "@repo-vector/parser-ts": "workspace:*",
    "@repo-vector/types": "workspace:*",
    "@repo-vector/vectorizer": "workspace:*"
  },
  "peerDependencies": {
    "ts-morph": "^25.0.1"
  }
}
